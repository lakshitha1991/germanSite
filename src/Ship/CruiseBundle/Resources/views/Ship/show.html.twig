{% extends '::indexLayout.html.twig' %}

{% block pannelTitle %}
    Ship
{% endblock %}
{% block content_table %}
    <table class="table table-striped table-bordered table-condensed table-responsive">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Ship</th>
                <td>{{ entity.ship }}</td>
            </tr>
        </tbody>
    </table>
{% endblock %}
{% block other %}
    <ul class="record_actions">
    <li>
        <a href="{{ path('ship') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('ship_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
